{
  "name": "deepkit-starter",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@7.3.0",
  "description": "A Deepkit Framework starter template",
  "license": "MIT",
  "author": "Nex Zhu <hitnexup@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/daotl/deepkit-starter.git"
  },
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --exit-child --respawn src/app.ts server:start",
    "start": "ts-node src/app.ts server:start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "refract-gen": "cd prisma/refract && npm run gen",
    "prisma-gen": "prisma generate",
    "prepare": "deepkit-type-install"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "prettier": "@daotl/prettier-config",
  "homepage": "https://github.com/daotl/deepkit-starter",
  "dependencies": {
    "@deepkit/app": "1.0.1-alpha.71",
    "@deepkit/broker": "1.0.1-alpha.71",
    "@deepkit/bson": "1.0.1-alpha.71",
    "@deepkit/core": "1.0.1-alpha.65",
    "@deepkit/core-rxjs": "1.0.1-alpha.65",
    "@deepkit/crypto": "1.0.1-alpha.65",
    "@deepkit/event": "1.0.1-alpha.71",
    "@deepkit/framework": "1.0.1-alpha.71",
    "@deepkit/http": "1.0.1-alpha.71",
    "@deepkit/injector": "1.0.1-alpha.71",
    "@deepkit/logger": "1.0.1-alpha.65",
    "@deepkit/orm": "1.0.1-alpha.71",
    "@deepkit/rpc": "1.0.1-alpha.71",
    "@deepkit/rpc-tcp": "1.0.1-alpha.71",
    "@deepkit/sql": "1.0.1-alpha.71",
    "@deepkit/sqlite": "1.0.1-alpha.71",
    "@deepkit/stopwatch": "1.0.1-alpha.71",
    "@deepkit/template": "1.0.1-alpha.71",
    "@deepkit/type": "1.0.1-alpha.71",
    "@deepkit/workflow": "1.0.1-alpha.71",
    "@graphql-yoga/node": "^2.9.2",
    "@prisma/client": "^3.15.2",
    "class-validator": "^0.13.2",
    "cross-undici-fetch": "^0.4.8",
    "graphql": "^15.8.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.17.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "tslib": "^2.4.0",
    "type-graphql": "1.2.0-rc.1"
  },
  "devDependencies": {
    "@daotl/eslint-config": "^0.3.25",
    "@daotl/tsconfig": "^0.1.0",
    "@deepkit/type-compiler": "1.0.1-alpha.71",
    "@types/graphql-fields": "^1.3.4",
    "@types/node": "^18.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.18.0",
    "prettier": "^2.7.1",
    "prisma": "^3.15.2",
    "prisma-dbml-generator": "^0.9.1",
    "prisma-erd-generator": "^0.11.4",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "typegraphql-prisma": "^0.20.4",
    "typescript": "^4.7.4"
  }
}
